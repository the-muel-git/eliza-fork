{
  "name": "eliza",
  "scripts": {
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply .",
    "preinstall": "npx only-allow pnpm",
    "build": "npx turbo run build --filter=!eliza-docs",
    "start": "pnpm --filter \"@elizaos/agent\" start --isRoot",
    "start:debug": "cross-env NODE_ENV=development VERBOSE=true DEFAULT_LOG_LEVEL=debug DEBUG=eliza:* pnpm --filter \"@elizaos/agent\" start --isRoot",
    "clean": "bash ./scripts/clean.sh",
    "test": "bash ./scripts/test.sh"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/jest": "^29.5.11",
    "cross-env": "7.0.3",
    "jest": "^29.7.0",
    "only-allow": "1.2.1",
    "typescript": "5.6.3"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "1.20.1",
      "@solana/web3.js@1.95.5": "npm:@solana/web3.js@1.95.5",
      "@solana/web3.js@1.95.8": "npm:@solana/web3.js@1.95.8",
      "@solana/web3.js@2": "npm:@solana/web3.js@2.0.0",
      "viem": "2.21.58",
      "@polkadot/util": "12.6.2",
      "@polkadot/util-crypto": "12.6.2",
      "@polkadot/types-create": "10.13.1",
      "@polkadot/types-codec": "10.13.1",
      "@polkadot/keyring": "12.6.2",
      "@ai-sdk/provider": "1.0.6",
      "@ai-sdk/provider-utils": "2.1.6",
      "cookie": "0.7.0",
      "bs58": "5.0.0",
      "@coral-xyz/anchor": "0.28.0"
    },
    "patchedDependencies": {
      "@solana-developers/helpers": "patches/@solana-developers__helpers.patch"
    }
  },
  "engines": {
    "node": "23.3.0"
  },
  "dependencies": {
    "@deepgram/sdk": "^3.9.0",
    "amqplib": "0.10.5",
    "bs58": "4.0.0",
    "langdetect": "^0.2.1",
    "ollama-ai-provider": "0.16.1",
    "pnpm": "9.15.0",
    "sharp": "0.33.5",
    "turbo": "2.3.3",
    "zod": "3.24.1"
  },
  "packageManager": "pnpm@9.15.0",
  "workspaces": [
    "packages/*"
  ]
}
